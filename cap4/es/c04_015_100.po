msgid "<CLT=cltSYSTEM>*knock, knock*"
msgstr "<CLT=cltSYSTEM>*toc, toc*"

msgctxt "0025 | chara_Blank"
msgid "<CLT=cltMIND>..."
msgstr "<CLT=cltMIND>..."

msgctxt "0026 | chara_Blank"
