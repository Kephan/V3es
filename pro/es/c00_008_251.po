msgid "<CLT=cltSYSTEM>"
msgstr "<CLT=cltSYSTEM>"

msgctxt "0057 | chara_Blank"
msgid "<CLT=cltSYSTEM>*click...click...*"
msgstr "<CLT=cltSYSTEM>*clac... clac...*"

msgctxt "0058 | C015_Akama"
msgid ""
"I guess this doesn't open...\n"
"Let's look somewhere else..."
msgstr ""
"No parece que se vaya a abrir...\n"
"Vamos a mirar a otro sitio..."

msgctxt "0059 | C000_Saiha"
msgid "I suppose so..."
msgstr "S-sí, vamos..."

msgctxt "0060 | chara_Blank"
msgid "<CLT=cltSYSTEM>"
msgstr ""
"<CLT=cltSYSTEM>¿Tú también lo has escuchado?\n"
"¡¿Una <CLT=cltSTRONG>mentira<CLT=cltSYSTEM>?!"

