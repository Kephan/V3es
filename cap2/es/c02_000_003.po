msgid "3F Lab B"
msgstr "Tercera planta, Sala B"

msgctxt "0004"
msgid "Maki's standing guard; I can't go in."
msgstr "Harukawa está haciendo guardia; no puedo entrar."

msgctxt "0005"
msgid "Gah-hahahahahahaha!"
msgstr "¡A-jajajajajajaja!"

msgctxt "0006"
